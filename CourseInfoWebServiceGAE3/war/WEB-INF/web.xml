<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>CourseInfoWebServiceGAE3</servlet-name>
		<servlet-class>com.webserviceGAE.courseinfo.CourseInfoWebServiceGAE3Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CourseInfoWebServiceGAE3</servlet-name>
		<url-pattern>/courseinfowebservicegae3</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addcourse</servlet-name>
		<servlet-class>com.webserviceGAE.courseinfo.AddCourseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addcourse</servlet-name>
		<url-pattern>/addcourse</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
